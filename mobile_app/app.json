{
  "expo": {
    "name": "eMessage",
    "description": "Dental office intra-communication app",
    "slug": "e-message",
    "privacy": "unlisted",
    "sdkVersion": "31.0.0",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "primaryColor": "#74CFEE",
    "icon": "./src/assets/images/logo-no-text.png",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#74CFEE"
    },
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "image": "./src/assets/images/full-logo.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.skylerbala.apps.emessage",
      "buildNumber": "1.0.0",
      "icon": "./src/assets/images/logo-no-text.png",
      "supportsTablet": true,
      "splash": {
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "image": "./src/assets/images/full-logo.png"
      },
    },
    "android": {
      "package": "com.skylerbala.apps.emessage",
      "versionCode": 1,
      "icon": "./src/assets/images/logo-no-text.png",
    },
  }
}